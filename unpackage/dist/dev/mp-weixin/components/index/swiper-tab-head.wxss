
.dl{
	border-bottom: 1rpx solid #EEEEEE;
	width: 100%;
	white-space: nowrap;
	line-height: 100rpx;
	height: 100rpx;
	border-bottom: 1px solid #c8c7cc;
}
.swiper-tab-list{
	color: #989798;
	font-weight: bold;
}
.uni-tab-bar .active{
	color: #1d1c1d;
}
.active .swiper-tab-line{
	border-bottom: 6rpx solid #ece579;
	width: 70rpx;
	margin: auto;
	border-top: 6rpx solid #FEDE33;
	border-radius: 20rpx;
}

