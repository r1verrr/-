
.nav-left>view, .nav-right>view{
	font-size: 45rpx;
}
.nav-left>view{
	color: #e9a459;
}
.nav-left{
	margin-left: 16rpx;
}
.nav-right{
		width: 100%;
}
.nav-tab-bar{
	width: 100%;
	margin-left: -20rpx;
	position: relative;
}
.nav-tab-bar>view{
	font-size: 33rpx;
	padding: 0 15rpx;
	font-weight: bold;
	color:#9fa09f
}
.active{
	color: #333333!important;
}
.nav-tab-bar-line{
	border-bottom: 5rpx solid #fdd12c;
	border-top: 5rpx solid #fdd12c;
	width:70rpx;
	border-radius: 20rpx;
	position: absolute;
	bottom:12rpx;
}


